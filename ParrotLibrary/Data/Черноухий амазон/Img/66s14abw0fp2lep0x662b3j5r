    function GetRndDogRscTeam()
    {
      var mass = [
        { 'href': 'http://ko-komanda.org/messages.aspx?TopicID=30', 'src': '/Content/Images/Ko_komanda/kok280x160.jpg', 'alt': 'Сообщество помощи кавказским овчаркам' },
        { 'href': 'http://ovcharkomanda.ru/', 'src': '/Content/Images/Ko_komanda/nok280x160.jpg', 'alt': 'Московская Овчар-команда' },       
        { 'href': 'https://helpdog.ru/forum/', 'src': '/Content/Images/Ko_komanda/amst280x160.jpg', 'alt': 'СТАФФОПИТЫ И ИХ МЕТИСЫ В ШЕРЕМЕТЬЕВО' },
        { 'href': 'http://www.rottkomanda.ru/forum', 'src': '/Content/Images/Ko_komanda/rot280x160.jpg', 'alt': 'Ротт-Команда' },
        { 'href': 'http://alabaidog.kamrbb.ru/', 'src': '/Content/Images/Ko_komanda/asia280x160.jpg', 'alt': 'Азиат Ко New Life' },
        { 'href': 'http://alabaihelp.kamrbb.ru/', 'src': '/Content/Images/Ko_komanda/ah280x160.jpg', 'alt': 'Алабай-Хэлп' }
      ];

      var index = Math.floor(Math.random() * mass.length);
      var elem = mass[index];

      if (document.getElementById('DogRscTeamAnch') == null)
        return;

      document.getElementById('DogRscTeamAnch').href = elem.href;
      var img = document.getElementById('DogRscTeamImg');
      img.src = elem.src;
      img.alt = elem.alt;
      img.title = elem.alt;
    }

    setTimeout(function () { GetRndDogRscTeam(); setInterval(GetRndDogRscTeam, 5000); }, 0);
  </script>

